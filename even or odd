#include<stdio.h>
#include<conio.h>
int main()
{
	int n,num[n],i;
	printf("enter the number of elements:");
	scanf("%d",&n);
	printf("enter the numbers:",n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&num[i]);
		}
		printf("even numbers in array are:");
		for(i=0;i<n;i++)
{
if(num[i]%2==0)
{
	printf("%d\n",num[i]);
}
}
printf("odd numbers are:");
for(i=0;i<n;i++)
{
	if(num[i]%2!=0)
	{
		printf("%d\n",num[i]);
	}
}
printf("\n");
return 0;
}
