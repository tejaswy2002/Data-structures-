#include<stdio.h>
void main()
{
	int a[20],i,n,j,temp;
	printf("enter valuve of n : ");
	scanf("%d",&n);
	for(i=0;i<n;i++) 
	{
		printf("enter element for a[%d]: ",i);
		scanf("%d",&a[i]);
		}
		printf("before sorting elements in the array are /n");
		for(i=0;i<n;i++)
		{
			printf("value of a[%d] = %d\n",i,a[i]);
		}
		for(i=0;i<n;i++)
		{
			temp=a[i];
			for(j=i-1;j>=0 && temp<a[j];j--)
			{
				a[j+1]=a[j];
				}
				printf("after sorting the elements in the array are\n");
				for(i=0;i<n;i++)
				{
					printf("value of a[%d] = %d\n",i,a[i]);
				}
